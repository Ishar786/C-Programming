// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    printf("\t\tBharath Acharya\n");
    printf("...............................\n");
    int i,j, arrmarks[6][4] = {47,7,5,0,53,6,10,0,69,8,7,0,48,9,8,0,55,5,6,0,46,7,6,0}, temp=0;
    for (i=0;i<6;i++)
    {
        temp = 0;
        //printf("enter marks for subject %d ", i+1);
        //printf("Theory: ");
        //scanf("%d", &arrmarks[i][0]);
        temp = temp + arrmarks[i][0];
        //printf("Viva: ");
        //scanf("%d", &arrmarks[i][1]);
        temp = temp + arrmarks[i][1];
        //printf("Practical: ");
        //scanf("%d", &arrmarks[i][2]);
        temp = temp + arrmarks[i][2];
        arrmarks[i][3] = temp;
    }
    printf("Semester Marks Sheet\n\n");
    printf(" Subject   Theory          Viva \t\t    Practical   \tTotal\t\n");
    printf("..................................................................\n");
    for (i=0;i<6;i++)
    {
        printf("subject %d", i+1);
        for (j=0;j<4;j++)
        {
            printf("\t\t%2d \t\t", arrmarks[i][j]);
        }
        printf("\n");
    }
    printf("..................................................................\n");
    
    
    printf("\n\n");
    printf("Sheik Ishar");
    return 0;
}
