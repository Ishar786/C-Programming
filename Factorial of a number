// Online C compiler to run C program online
#include <stdio.h>
#include<math.h>

int main() 
{
    int num, fact=1, temp;
    printf("Factorial of a number\n\n");
    printf("enter the number: ");
    scanf("%d", &num);
    temp = num;
    if (num!=0)
    {
        do
        {
            fact = fact*num;
            num = num-1;
        } while (num>0);
    }
    printf("factorial of %d is %d\n\b", temp, fact);
   
    printf("\n\n");
    printf("Sheik Ishar");
    return 0;
}
