#include <stdio.h>

int main() 
{
    float a;
    printf("enter the number: ");
    scanf("%f", &a);
    if (a>0)
    {
        printf("%f is positive", a);
    }
    else if (a<0)
    {
        printf("%f is negative", a);
    }
    else
    {
        printf("%d is equal to 0",a);
    }
    printf("\n\n");
    printf("Sheik Ishar");
    return 0;
}
