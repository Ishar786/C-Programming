#include <stdio.h>
#include<math.h>

int main() 
{
    float i;
    for (i=1;i<1.02;i=i+(i*0.01))
    {
       printf("%4.2f\n", pow(i,365));
        
    }
    printf("\n\n");
    printf("Sheik Ishar");
    return 0;
}
//second program

#include <stdio.h>
#include<math.h>

int main() 
{
    int i, num, count;
    printf("enter a number: ");
    scanf("%d", &num);
    printf("what should be the count: ");
    scanf("%d", &count);
    for (i=0;i<count;i++)
    {
       printf("%d * %d\t = %d\n", num, i+1, num*(i+1));
        
    }
    printf("\n\n");
    printf("Sheik Ishar");
    return 0;
}
