// Online C compiler to run bubble sorting algorithm for ascending order of arrays program online
#include <stdio.h>
#include<stdlib.h>

int main() 
{
    int i,j, count, temp;
    int arrnum[20];
    printf("enter the number of elements: ");
    scanf("%d", &count);
    printf("enter %d numbers......", count);
    for (i=0;i<count;i++)
    {
        printf("\n[%d]: ", i+1);
        scanf("%d", &arrnum[i]);   
    }
    printf("the given array is......\n");
    for(i=0;i<count;i++)
    {
        printf("%d ", arrnum[i]);
    }
    for(i=0;i<count-1;i++)
    {
        for(j=0;j<count-1;j++)
        {
            if (arrnum[j]>arrnum[j+1])
            {
                temp = arrnum[j];
                arrnum[j] = arrnum[j+1];
                arrnum[j+1] = temp;
            }
        }
    }
    printf("\nsorted ascending order is.......\n");
    for (i=0;i<count;i++)
    {
        printf("%d ", arrnum[i]);   
    }
    printf("\n\n");
    printf("Sheik Ishar");
    return 0;
}
